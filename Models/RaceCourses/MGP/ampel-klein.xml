  	  </equals>
  	  <equals>
         <property>/startsignal</property>
  	   <value>5</value>
  	  </equals>
      </or>
  </condition>
</animation>

<animation>
  <type>select</type>
	 <object-name>red.003</object-name>
  <condition>
  	<or>
  	  <equals>
         <property>/startsignal</property>
  	   <value>3</value>
  	  </equals>
  	  <equals>
         <property>/startsignal</property>
  	   <value>4</value>
  	  </equals>
  	  <equals>
         <property>/startsignal</property>
  	   <value>5</value>
  	  </equals>
      </or>
  </condition>
</animation>

<animation>
  <type>select</type>
	 <object-name>red.004</object-name>
  <condition>
  	<or>
  	  <equals>
         <property>/startsignal</property>
  	   <value>4</value>
  	  </equals>
  	  <equals>
         <property>/startsignal</property>
  	   <value>5</value>
  	  </equals>
      </or>
  </condition>
</animation>

